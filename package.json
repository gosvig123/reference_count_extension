{
  "name": "css-class-counter",
  "displayName": "Reference Counter",
  "description": "Counts and displays the usage of references in your code",
  "version": "0.0.14",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:html",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onCommand:css-class-counter.countClasses",
    "onLanguage:python"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.displayReferenceCount",
        "title": "Display Reference Count"
      },
      {
        "command": "extension.showFunctionList",
        "title": "Show Function List"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/react": "^18.3.3",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "publisher": "gosvig123",
  "repository": {
    "type": "git",
    "url": "https://github.com/gosvig123/reference_count_extension"
  },
  "license": "MIT",
  "dependencies": {
    "minimatch": "^10.0.1",
    "react": "^18.3.1"
  }
}
